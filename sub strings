#include <stdio.h>
#include<string.
int main()
{
    int i,j,k;
    char s[100];
    scanf("%s",s);
    int n = strlen(string);
for(int i = 0; i < n; i++)
{   for(int j = i; j < n; j++)
    {   /* print substring from i to j */
        for(int k = i; k <= j; k++)
        {   printf("%c", string[k]);
        }
        printf("\n");
    }
}
